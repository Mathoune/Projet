#include <stdio.h>
#include <stdlib.h>
#include "mes_types.h"
#include "lire_ecrire.h"
#include "afficher.h"
#include "listes_memo.h"
#include "jeu.h"

int
main (int argc, char *argv[])
{
  printf ("Debut slider\n");
  SLIDER S;
  LISTE l=NULL;
  S = init_slider (argv[1]);
  afficher_slider (S);

  l = bouge (S,l);

  printf ("Fin slider\n");
  finir_affichage (S);
  libere_liste(l);
  libere_murs(S);

  exit (0);
}
